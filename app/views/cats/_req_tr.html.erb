<tr>
  <td><%= User.find_by(id: req.user_id).user_name %></td>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if req.pending? && cat.user.id == User.find_by(session_token: self.session[:session_token]).id %>
        <%= button_to "Approve", approve_cat_rental_request_url(req) %>
        <%= button_to "Deny", deny_cat_rental_request_url(req) %>
    <% else %>
      <%= req.status %>
    <% end %>
  </td>
</tr>
